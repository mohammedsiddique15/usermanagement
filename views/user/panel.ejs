<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Panel</title>
</head>
<body>
    <h1>Admin Panel</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Verified</th>
            </tr>
        </thead>
        <tbody>
            <% if (users && users.length > 0) { %>
                <% users.forEach(user => { %>
                    <tr>
                        <td><%= user.name %></td>
                        <td><%= user.email %></td>
                        <td><%= user.phoneNumber %></td>
                        <td><%= user.isVerified ? 'Yes' : 'No' %></td>
                    </tr>
                <% }) %>
            <% } else { %>
                <tr>
                    <td colspan="4">No users found</td>
                </tr>
            <% } %>
        </tbody>
    </table>
</body>
</html>
